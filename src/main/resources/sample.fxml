<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.Group?>
<GridPane fx:controller="ui.sample.Controller"
          stylesheets="sample.css"
          fx:id="grid"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <Button GridPane.rowIndex="1" text="Start" onAction="#startGame"/>
    <Button GridPane.rowIndex="2" text="Stop" onAction= "#stopGame"/>
    <Group GridPane.rowIndex="3" fx:id="group"/>
</GridPane>

